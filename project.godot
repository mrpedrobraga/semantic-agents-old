; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GDSK"
config/description="Integration of the Semantic Kernel API with the Godot Game Engine for the creation of AI-powered entities and the like."
config/tags=PackedStringArray("somedude")
run/main_scene="res://node_2d.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=256
window/size/viewport_height=256
window/size/window_width_override=512
window/size/window_height_override=512
window/stretch/mode="viewport"

[dotnet]

project/assembly_name="Ai"

[editor_plugins]

enabled=PackedStringArray("res://addons/ai_semantic_agents/plugin.cfg", "res://addons/plugin_refresher/plugin.cfg")

[rendering]

textures/canvas_textures/default_texture_filter=0
textures/canvas_textures/default_texture_repeat=1
